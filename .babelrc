{
  "presets": [
    [
      "stage-2"
    ],
    [
      "es2015",
      {
        "modules": false
      }
    ]
  ],
  "plugins": [
    "transform-vue-jsx",
    [
      "babel-plugin-root-import",
      {
        "rootPathPrefix": "@"
      }
    ]
  ],
  "env": {
    "test": {
      "plugins": [
        "istanbul"
      ]
    }
  }
}
